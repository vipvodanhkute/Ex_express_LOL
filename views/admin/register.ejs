<%- include('../_layout/admin_header') %>
<form action="/admin/users/register" method="post">
    <div class="form-group">
        <label for="">Name</label>
        <input type="text" placeholder="Name" class="form-control" name="name">
    </div>
    <div class="form-group">
        <label for="">UserName</label>
        <input type="text" placeholder="User Name" class="form-control" name="username">
    </div>
    <div class="form-group">
        <label for="">Password</label>
        <input type="password" placeholder="Password" class="form-control" name="password">
    </div>
    <div class="form-group">
        <label for="">ConfirmPassword</label>
        <input type="password" placeholder="Confirm Password" class="form-control" name="password2">
    </div>
    <button class="btn btn-primary" type="submit">Add</button>
</form>
<%- include('../_layout/admin_footer') %>